<template name="index$7c5e7167">
  <view class="_div data-v-148b6335 addressAddContainer">
    <view class="_div data-v-148b6335 addressAddContent">
      <view class="_div data-v-148b6335 addressAddContentList">
        <view class="_div data-v-148b6335 addressAddContentListTitle">联系人</view>
        <input class="_input data-v-148b6335 addressAddContentListInput" type="text" value="{{addressUser.addressName}}" bindinput="handleProxy" autofocus placeholder="名字" data-eventid="{{'0'}}" data-comkey="{{$k}}" />
      </view>
      <view class="_div data-v-148b6335 addressAddContentList">
        <view class="_div data-v-148b6335 addressAddContentListTitle">联系电话</view>
        <input class="_input data-v-148b6335 addressAddContentListInput" type="number" value="{{addressUser.addressPhone}}" bindinput="handleProxy" placeholder="请输入手机号" data-eventid="{{'1'}}" data-comkey="{{$k}}" />
      </view>
      <view class="_div data-v-148b6335 addressAddContentList">
        <view class="_div data-v-148b6335 addressAddContentListTitle">详细地址</view>
        <textarea class="_textarea data-v-148b6335 addressAddContentListInput" value="{{addressUser.addressDetail}}" bindinput="handleProxy" maxlength="30" placeholder="请输入详细地址(最多50字)" data-eventid="{{'2'}}" data-comkey="{{$k}}" />
      </view>
    </view>
    <view wx:if="{{messageStatus}}" class="_p data-v-148b6335 message">您还没有填完信息哦~</view>
    <view class="_div data-v-148b6335 operatorButton">
      <view class="_div data-v-148b6335 addressAddButton del" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}">删除</view>
      <view class="_div data-v-148b6335 addressAddButton save" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">修改</view>
    </view>
    <view class="_div data-v-148b6335 delContainer" wx:if="{{tipsMessageDel}}">
      <view class="_div data-v-148b6335 delFull">
        <view class="_div data-v-148b6335 delContent">
          <view class="_div data-v-148b6335 delContentTitle"> 确定删除该收货地址吗？ </view>
          <view class="_div data-v-148b6335 delContentButton">
            <view class="_div data-v-148b6335 delContentButtonL" bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">取消</view>
            <view class="_div data-v-148b6335 delContentButtonR" bindtap="handleProxy" data-eventid="{{'6'}}" data-comkey="{{$k}}">确定</view>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>