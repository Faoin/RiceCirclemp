            <view class="_div data-v-29881e2c shopActivityicon clearflx">
              <view class="_div data-v-29881e2c shopActivityiconJ">折</view>
              <view class="_p data-v-29881e2c">{{item.fold}}</view>
            </view>
          </view>
        </view>
      </view>
      <view class="_div data-v-29881e2c">
        <i-load-more tip="暂无数据" loading="{{loadingShow}}" class="_i-load-more data-v-29881e2c"></i-load-more>
      </view>
    </view>
  </view>
</template>