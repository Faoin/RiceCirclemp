              </view>
            </navigator>
            <view class="_div data-v-29881e2c sCTDetail">
              <view class="_div data-v-29881e2c sCTDetailTitle">
                <view class="_div data-v-29881e2c sCTDetailTitleicon">店铺</view>
                <view class="_p data-v-29881e2c">{{item.title}}</view>
              </view>
              <view class="_div data-v-29881e2c sCTDetailpj"> {{item.sales}} </view>
              <view class="_div data-v-29881e2c sCTDetailm"> {{item.distribution}} </view>
            </view>
          </view>
          <view class="_div data-v-29881e2c shopLine"></view>
          <view class="_div data-v-29881e2c shopActivity clearflx">
            <view class="_div data-v-29881e2c shopActivityicon clearflx">
              <view class="_div data-v-29881e2c shopActivityiconJ">减</view>
