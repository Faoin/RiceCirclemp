        </view>
        <view class="_div data-v-0fb92a52 shoppListDetailD">
          <view class="_p data-v-0fb92a52 shoppListDetailDT">{{itemList.shoppListDetailDT}}</view>
          <view class="_p data-v-0fb92a52 shoppListDetailDP">{{itemList.shoppListDetailDP}}</view>
          <view class="_p data-v-0fb92a52 shoppListDetailDM">{{itemList.shoppListDetailDM}}</view>
        </view>
      </view>
    </view>
    <view class="_div data-v-0fb92a52 {{[loadingShow ? 'shoppLoading' : '']}}">
      <i-load-more tip="暂无数据" loading="{{loadingShow}}" class="_i-load-more data-v-0fb92a52"></i-load-more>
    </view>
  </view>
</template>