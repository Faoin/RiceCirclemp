      <view class="_div data-v-0fb92a52 shoppTitle">
        <view class="_p data-v-0fb92a52 shoppTitleL">美食推荐</view>
      </view>
    </view>
    <view class="_div data-v-0fb92a52 shoppList" wx:key="index" key="{{index}}" wx:for="{{newShopp}}" wx:for-index="index" wx:for-item="item">
