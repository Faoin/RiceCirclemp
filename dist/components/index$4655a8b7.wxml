<template name="index$4655a8b7">
  <view class="_div data-v-495e39f6 commodityContainer">
    <view class="_div data-v-495e39f6 commodity">
      <image src="/static/img/banner02.jpg" alt="cb" class="_img data-v-495e39f6"></image>
      <view class="_div data-v-495e39f6 commodityLogo">
        <image src="/static/img/youtube.jpg" alt="xbk" class="_img data-v-495e39f6"></image>
      </view>
    </view>
    <view class="_div data-v-495e39f6 commodityTitle">{{commodityName}}</view>
    <view class="_div data-v-495e39f6 commodityDetail">10元起送|免费配送</view>
    <view class="_div data-v-495e39f6 commodityList"> 点餐 </view>
    <view class="_div data-v-495e39f6 commodityListShopp">
      <view class="_div data-v-495e39f6 commodityListShoppMenu">
        <view class="_ul data-v-495e39f6">
          <view bindtap="handleProxy" wx:key="index" key="{{index}}" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}" wx:for="{{commodityList}}" wx:for-index="index" wx:for-item="item" class="_li data-v-495e39f6 {{[index === commodityIndexs ? 'active' : '']}}">{{item.commodityMenu}}</view>
        </view>
      </view>
      <view class="_div data-v-495e39f6 commodityListShoppContent">
        <view class="_div data-v-495e39f6 commodityToggleList {{[commodityIndex === commodityIndexs ? 'commodityToggle' : '']}}" wx:key="commodityIndex" key="{{commodityIndex}}" wx:for="{{commodityList}}" wx:for-index="commodityIndex" wx:for-item="commodityItem">
          <view bindtap="handleProxy" class="_div data-v-495e39f6 commodityListShoppContentD" wx:key="commodityIndexs" key="{{commodityIndexs}}" data-eventid="{{'1-'+commodityIndex+'-'+commodityIndexs}}" data-comkey="{{$k}}" wx:for="{{commodityItem.commodityData}}"
            wx:for-index="commodityIndexs" wx:for-item="commodityItems">
            <view class="_div data-v-495e39f6 commodityListShoppContentDImg">
              <image src="{{commodityItems.commodityImg}}" alt="{{commodityItems.commodityAlt}}" class="_img data-v-495e39f6"></image>
            </view>
            <view class="_div data-v-495e39f6 commodityListShoppContentDD">
              <view class="_p data-v-495e39f6 commodityListShoppContentDDT">{{commodityItems.commodityName}}</view>
              <view class="_p data-v-495e39f6 commodityListShoppContentDDD">{{commodityItems.commodityDetail}}</view>
              <view class="_p data-v-495e39f6 commodityListShoppContentDDD">{{commodityItems.commodityDetail2}}</view>
              <view class="_p data-v-495e39f6 commodityListShoppContentDDM">{{commodityItems.commodityMoney}}元</view>
              <view class="_div data-v-495e39f6 commodityAdd">
                <image src="/static/img/add.png" alt="add" class="_img data-v-495e39f6"></image>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
    <view class="_div data-v-495e39f6 commodityemtyp"></view>
    <view class="_div data-v-495e39f6 commodityShoppList {{[commodityToggleShow ? 'commodityShoppListToggle' : '']}}">
      <i-cell-group class="_i-cell-group data-v-495e39f6">
        <i-cell title="{{item.commodityName}}" wx:key="index" key="{{index}}" wx:for="{{commdityShopping}}" wx:for-index="index" wx:for-item="item" class="_i-cell data-v-495e39f6">
          <view slot="footer" class="_div data-v-495e39f6">
            <view class="_div data-v-495e39f6 commodityShoppListM">{{item.commodityMoney * item.commoditySum}}元</view>
            <view class="_div data-v-495e39f6 commodityShoppListSwitch">
              <view class="_div data-v-495e39f6 commodityShoppListSwitchLess" bindtap="handleProxy" data-eventid="{{'2-'+index}}" data-comkey="{{$k}}">－</view>
              <view class="_div data-v-495e39f6 commodityShoppListSwitchSum">{{item.commoditySum}}</view>
              <view class="_div data-v-495e39f6 commodityShoppListSwitchAdd" bindtap="handleProxy" data-eventid="{{'3-'+index}}" data-comkey="{{$k}}">＋</view>
            </view>
          </view>
        </i-cell>
        <i-cell title="配送费" class="_i-cell data-v-495e39f6">
          <view slot="footer" class="_div data-v-495e39f6"> {{commodityPrice >= 10 ? 0 : 1}}元 </view>
        </i-cell>
      </i-cell-group>
    </view>
    <view class="_div data-v-495e39f6 commoditySum {{[commoditySumShopp ? 'commoditySumStatus' : '']}}" bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">
      <view class="_div data-v-495e39f6 commodityG {{[commoditySumShopp ? 'commodityGStatus' : '']}}">
        <i-icon type="publishgoods_fill" size="40" color="{{ commoditySumShopp === 0 ?  '#5f5f63' : '#ffffff' }}" class="_i-icon data-v-495e39f6"></i-icon>
        <view wx:if="{{commoditySumShopp}}" class="_div data-v-495e39f6 commodityGsum">{{commoditySumShopp}}</view>
      </view>
      <view class="_div data-v-495e39f6 commodityD">
        <view class="_p data-v-495e39f6 commodityDM {{[commoditySumShopp ? 'commodityDMStatus' : '']}}"> {{ commoditySumShopp === 0 ? '未选购商品' : commodityPrice + '元' }} </view>
        <view class="_p data-v-495e39f6 commodityDD"> 不足十元需要加一元配送费 </view>
      </view>
      <view class="_div data-v-495e39f6 commodityM {{[commoditySumShopp ? 'commodityMStatus' : '']}}" catchtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}"> {{ commoditySumShopp === 0 ? '10元起送' : '去结算'}} </view>
    </view>
    <view bindtap="handleProxy" class="_div data-v-495e39f6 commodityFull {{[commodityToggleShow ? 'commodityFullToggle' : '']}}" data-eventid="{{'6'}}" data-comkey="{{$k}}"></view>
  </view>
</template>